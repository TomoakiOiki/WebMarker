{
  "manifest_version": 3,
  "name": "WebMarker",
  "version": "1.0",
  "description": "A Chrome extension built with Vite and React",
  "background": {
    "service_worker": "dist/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"]
    }
  ]
}
